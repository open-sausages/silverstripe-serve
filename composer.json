{
    "name": "silverstripe/serve",
    "description": "Connects the PHP development server to SilverStripe",
    "license": "MIT",
    "require": {
        "php": "^5.6||^7.0",
        "symfony/process": "^3.0",
        "silverstripe/framework": "4.0.x-dev",
        "silverstripe/config": "1.0.x-dev"
    },
    "require-dev": {
    	"phpunit/phpunit": "^4.8 || ^5.7"
    },
    "autoload": {
        "psr-4": {
        	"SilverStripe\\Serve\\" : "code/"
        }
    },
    "bin": ["bin/serve"],
    "minimum-stability": "dev",
    "prefer-stable": true
}
